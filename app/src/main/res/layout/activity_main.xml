<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:padding="5dp"
        android:paddingStart="10dp"
        android:text="@string/textview2"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.886" />

    <LinearLayout
        android:layout_width="349dp"
        android:layout_height="530dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        tools:layout_editor_absoluteX="31dp">


        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="40dp"
            android:layout_height="35dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="60dp"
            app:srcCompat="@drawable/indo" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:text="Name "
            android:textSize="16sp" />


        <EditText
            android:id="@+id/editTextTextPersonName2"
            android:layout_width="141dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="Enter your Name here"
            android:inputType="textPersonName" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="License No."
            android:textSize="16sp" />

        <EditText
            android:id="@+id/editTextTextPersonName3"
            android:layout_width="144dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="Enter Valid Number"
            android:inputType="textPersonName"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="180dp"
            android:layout_marginTop="-70dp"
            android:text="Authorisation to Drive"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/editTextTextPersonName6"
            android:layout_width="155dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="180dp"
            android:ems="10"
            android:inputType="textPersonName" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Date of Issue"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/editTextDate"
            android:layout_width="134dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:inputType="date" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="180dp"
            android:layout_marginTop="-70dp"
            android:text="Valid Till"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/editTextDate3"
            android:layout_width="133dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="180dp"
            android:ems="10"
            android:inputType="date" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="60dp"
            android:layout_marginTop="-275dp"
            android:text="@string/textview8" />

        <androidx.constraintlayout.widget.Barrier
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="1"
            app:barrierDirection="bottom" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_marginTop="280dp"
            android:text="Date of Birth"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_marginStart="180dp"
            android:layout_marginTop="-20dp"
            android:text="S/W/D" />

        <EditText
            android:id="@+id/editTextDate2"
            android:layout_width="135dp"
            android:layout_height="40dp"
            android:layout_marginTop="10dp"
            android:ems="10"
            android:inputType="date" />

        <EditText
            android:id="@+id/editTextTextPersonName"
            android:layout_width="135dp"
            android:layout_height="40dp"
            android:layout_marginStart="180dp"
            android:layout_marginTop="-42dp"
            android:ems="10"
            android:inputType="textPersonName" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            app:srcCompat="@drawable/barcode" />

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="70dp"
            android:layout_height="60dp"
            android:layout_marginStart="180dp"
            android:layout_marginTop="-60dp"
            app:srcCompat="@drawable/selfpic" />

    </LinearLayout>

    <Button
        android:id="@+id/button"
        android:layout_width="177dp"
        android:layout_height="47dp"
        android:text="Done"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="668dp" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:alpha="1"
        app:barrierDirection="top"
        app:constraint_referenced_ids="textView8,textView3"
        tools:ignore="NotSibling"
        tools:layout_editor_absoluteX="411dp"
        tools:layout_editor_absoluteY="731dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="190dp"
        android:layout_height="65dp"
        android:background="#8D53F2"
        android:padding="8dp"
        android:paddingStart="60dp"
        android:text="@string/textview"
        android:textColor="#FFFFFF"
        android:textSize="22sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="28dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="63dp"
        android:layout_height="55dp"
        app:srcCompat="@drawable/cloud_upload"
        tools:ignore="VectorDrawableCompat"
        tools:layout_editor_absoluteX="103dp"
        tools:layout_editor_absoluteY="38dp" />

</androidx.constraintlayout.widget.ConstraintLayout>